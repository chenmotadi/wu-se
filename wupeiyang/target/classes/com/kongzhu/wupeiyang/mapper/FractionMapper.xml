<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kongzhu.wupeiyang.mapper.FractionMapper">
	<select id="selectScore" parameterType="com.kongzhu.wupeiyang.entity.Fraction" resultType="int" >
	select score from fraction  where  student_id=#{studentId} and course_id=#{courseId}
	</select>
</mapper>